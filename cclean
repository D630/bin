#!/bin/sh
#
# Clean up some dirs and files.

rm -frv -- \
    "$HOME/".adobe \
    "$HOME/".cache \
    "$HOME/".gconf \
    "$HOME/".git-credential-cache \
    "$HOME/".gnome2 \
    "$HOME/".gnome2_private \
    "$HOME/".icedteaplugin \
    "$HOME/".macromedia \
    "$HOME/".thumbnails \
    "${XDG_CONFIG_HOME:?}/"freshwrapper-data;

rm -fv -- \
    "$HOME/".gksu.lock \
    "$HOME/".newsbeuter/newsbeuter_cache.db \
    "$HOME/".recentf \
    "$HOME/".recently-used \
    "${XDG_DATA_HOME:?}/"recently-used.xbel;

