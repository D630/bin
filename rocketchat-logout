#!/bin/sh
#
# Logout me out from rocketchat via REST.

auth_token_file=$HOME/.rocketchat_wir_invalid_auth_token;

read -r ROCKETCHAT_REST_TOKEN < "$auth_token_file" ||
	exit 1;

ROCKETCHAT_REST_USER_ID=z6O73D6HJPjP9viLD;

export \
	ROCKETCHAT_REST_TOKEN \
	ROCKETCHAT_REST_USER_ID;

rocketchat-rest-api post /api/v1/logout 2>/dev/null |
jq -r .;

rm "$auth_token_file";

# vim: set ft=sh :
